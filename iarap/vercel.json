{
    "version": 2,
    "builds": [
        {
            "src": "iarap/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "iarap/wsgi.py"
        }
    ],
    "env": {
        "DEBUG": "False",
        "ENVIRONMENT": "production"
    }
}